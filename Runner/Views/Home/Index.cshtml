@using (Html.BeginForm())
{

    <head>
        <title>Lock/Unlock</title>
        <link rel="stylesheet" type="text/css" href="styles.css">
        <link rel="stylesheet" type="text/css" href="jquery-ui.min.css">
        <script src="underscore-min.js"></script>
        <script src="jquery-2.1.3.min.js"></script>
        <script src="jquery-ui.min.js"></script>
        <script src="parse.js"></script>
        <script src="manageAccess2.js"></script>
    </head>
    <template id="branch">
        <div class="branch">
            <div class="deny">
                <input type="checkbox" id="<%= name %>">
                <label for="<%= name %>"><%= name %></label>
            </div>
            <div class="allowWrapper">
                <input type="text" class="userName" data-allow="<%= name %>">
            </div>
        </div>
    </template>
    <body>
        <div id="manageAccess">
            <div id="branchesWrappe">
                <div class="titles">
                    <h3 class="denyTitle">Deny access to next branches:</h3>
                    <h3>Allow access to next branches:</h3>
                </div>
                <div id="branches"></div>
            </div>



            <div class="resultWrapper">
                <h4>
                    Result:<h4>
                        <pre id="result"></pre>
            </div>
            <div class="mocks">

                <label> File Content: </label>
                <div>
                    <pre class="acccessMocks">
                @Html.EditorFor(model => Model.FileContent, new { style = "width: 100%;" })
				</pre>

                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Submit" class="btn btn-default" />
                    </div>
                </div>



                <label> cmd result: </label>
                <div class='content'>
                    <pre class="branchesMocks">
                    @Html.DisplayFor(model => model.CommandResult)
              
</pre>
                </div>

            </div>
        </div>
    </body>

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}